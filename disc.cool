server {
  listen 80 default_server;
  listen [::]:80 default_server;
  root /usr/local/var/www/disc.cool/;
  index index.html;
  server_name disc.cool www.disc.cool;
  location ~ ^/api/(.*)$  {
	# insert Apollo IP address and server port number below
    	proxy_pass http://192.168.1.90:9999/$1;
  }
}
